    <style>
      html, body, #map-canvas {
        height: 100%;
        margin: 0px;
        padding: 0px
      }
    </style>
    <script src="https://maps.googleapis.com/maps/api/js?v=3.exp"></script>
    <script>
// Create an object containing LatLng and radius for each city or land mark.
//radius in miles
var citymap = {};
citymap['@Brooklyn'] = {
  center: new google.maps.LatLng(40.650002, -73.949997),
  radius: 8368.568
};
citymap['@Soho'] = {
  center: new google.maps.LatLng(40.723384, -74.001704),
  radius: 965.604
};
citymap['@Manhattan'] = {
  center: new google.maps.LatLng(40.783435, -73.966249),
  radius: 6759.228
};

citymap['@Times Square'] = {
  center: new google.maps.LatLng(40.7560982, -73.98578809),
  radius: 1287.472
};

citymap['@Central Park'] = {
  center: new google.maps.LatLng(40.7819, -73.96336),
  radius: 1770.274
};
citymap['@Long Beach'] = {
  center: new google.maps.LatLng(40.5884373,	-73.657908),
  radius: 1448.406
};
citymap['@New York'] = {
  center: new google.maps.LatLng(40.71000, -74.01000),
  radius: 20921.42
};
citymap['@Queens'] = {
  center: new google.maps.LatLng(40.73,	-73.88666),
  radius: 5471.756
};
citymap['@Bronx'] = {
  center: new google.maps.LatLng(40.844781,	-73.864826),
  radius: 5471.756
};
citymap['@Staten Island'] = {
  center: new google.maps.LatLng(40.5795317,	-74.1502007),
  radius: 6759.228
};
citymap['@Ellis Island'] = {
  center: new google.maps.LatLng(40.6984721,	-74.0399595),
  radius: 354.0548
};
citymap['@Liberty Island'] = {
  center: new google.maps.LatLng(40.844781,	-73.864826),
  radius: 257.4944
};
citymap['@Brooklyn Bridge'] = {
  center: new google.maps.LatLng(40.7060855,	-73.9968642),
  radius: 482.802
};
citymap['@Coney Island'] = {
  center: new google.maps.LatLng(40.564926,	-73.975941),
  radius: 3540.548
};
citymap['@5th Avenue'] = {
  center: new google.maps.LatLng(40.6603,	-73.9945),
  radius: 2092.142
};
var cityCircle;
function initialize() {
  // Create the map.
  var mapOptions = {
    zoom: 12,
    center: new google.maps.LatLng(21.461390, -157.985828),
    mapTypeId: google.maps.MapTypeId.ROADMAP
  };
  var map = new google.maps.Map(document.getElementById('map-canvas'),
      mapOptions);
  // Construct the circle for each value in citymap.
  for (var city in citymap) {
    var circleOptions = {
      strokeColor: '#d45b5b',
      strokeOpacity: 0.8,
      strokeWeight: 2,
      fillColor: '#d45b5b',
      fillOpacity: 0.35,
      map: map,
      center: citymap[city].center,
      //radius: Math.sqrt(citymap[city].population) * 100
      radius: citymap[city].radius
    };
    // Add the circle for this city to the map.
    cityCircle = new google.maps.Circle(circleOptions);
  }
}
google.maps.event.addDomListener(window, 'load', initialize);
    </script>
  </head>
